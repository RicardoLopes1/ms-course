root = true

[*]
end_of_line = lf
charset = utf-8
indent_size = 4
indent_style = space
insert_final_newline = true
trim_trailing_whitespace = true
ij_any_blank_lines_after_package = 1
ij_any_blank_lines_after_imports = 1
ij_any_blank_lines_after_class_header = 1
ij_any_blank_lines_around_field = 1
ij_any_blank_lines_before_class_end = 1
max_line_length = 150

[*.yml]
indent_size = 2
